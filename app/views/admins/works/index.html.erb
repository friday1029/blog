<p id="notice"><%= notice %></p>

<h1>Admins Works</h1>

<table>
  <thead>
    <tr>
      <th>Name zh</th>
      <th>Name en</th>
      <th>Site url</th>
      <th>Desc zh</th>
      <th>Desc en</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @works.each do |work| %>
      <tr>
        <td><%= work.name_zh %></td>
        <td><%= work.name_en %></td>
        <td><%= work.site_url %></td>
        <td><%= work.desc_zh %></td>
        <td><%= work.desc_en %></td>
        <td><%= link_to 'Show', admins_work_path(work) %></td>
        <td><%= link_to 'Edit', edit_admins_work_path(work) %></td>
        <td><%= link_to 'Destroy', admins_work_path(work), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admins Work', new_admins_work_path %>
